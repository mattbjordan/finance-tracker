{
  "name": "finance-tracker",
  "version": "0.0.0",
  "description": "Tool for tracking transactions/spending and maybe more",
  "main": "./lib/main.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:ts && npm run build:copy_public",
    "build:ts": "tsc",
    "build:copy_public": "cp ./public/** ./lib",
    "clean": "rimraf lib",
    "clean:node": "rimraf node_modules",
    "start": "node ./lib/main.js",
    "start:page": "node ./server.cjs",
    "cbs": "npm run clean && npm run build && npm run start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [],
  "author": "",
  "devDependencies": {
    "@types/jsdom": "^21.1.3",
    "@types/node": "^20.8.3",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.51.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "dotenv": "^16.3.1",
    "jsdom": "^22.1.0",
    "plaid": "^16.0.0"
  }
}
